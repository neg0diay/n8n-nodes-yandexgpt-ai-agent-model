{
	"name": "n8n-nodes-yandexgpt-ai-agent-model",
	"version": "0.0.2",
	"description": "Yandex GPT nodes for n8n (ai agent model)",
	"keywords": [
		"n8n-community-node-package",
		"n8n-nodes-yandexgpt-ai-agent-model",
		"yandexgpt"
	],
	"license": "MIT",
	"homepage": "https://github.com/neg0diay/n8n-nodes-yandexgpt-ai-agent-model",
	"author": {
		"name": "neg0diay",
		"email": "neg@front-tech.ru"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/neg0diay/n8n-nodes-yandexgpt-ai-agent-model"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "index.js",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/YandexGptApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/LmYandexGpt/LmYandexGpt.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.82.0",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"@langchain/core": "^0.3.27",
		"@langchain/yandex": "^0.1.0",
		"n8n-core": "^1.14.1",
		"n8n-workflow": "^1.20.0"
	}
}
